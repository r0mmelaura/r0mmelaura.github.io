<html>
<head>
    <title>HTA Calculator Launcher</title>
    <script language="VBScript">
        Sub LaunchCalc()
            Dim objShell
            Set objShell = CreateObject("WScript.Shell")
            objShell.Run "calc.exe"
            Set objShell = Nothing
        End Sub
    </script>
</head>
<body onload="LaunchCalc()">
</body>
</html>
